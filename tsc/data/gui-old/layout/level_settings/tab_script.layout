<?xml version="1.0" encoding="UTF-8"?>

<GUILayout>
  <Window Type="DefaultWindow" Name="level_settings_tab_script">
    <Property Name="Text" Value="Script"/>
    <Property Name="InheritsAlpha" Value="False"/>
    <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
    <Property Name="UnifiedAreaRect"
	      Value="{{0,0},{0,0},{1,0},{1,0}}"/>
    <Window Type="TaharezLook/MultiLineEditbox" Name="multieditbox_script">
      <Property Name="Text" Value="# Your script code here"/>
      <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
      <Property Name="UnifiedAreaRect" Value="{{0.01,0},{0.01,0},{0.99,0},{0.92,0}}"/>
    </Window>
    <Window Type="TaharezLook/StaticText" Name="script_reload_info">
      <Property Name="Text" Value="NOTE: Changing the script code will force its re-evaluation!"/>
      <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}"/>
      <Property Name="UnifiedAreaRect" Value="{{0.01,0},{0.93,0},{0.99,0},{0.99,0}}"/>
    </Window>
  </Window>
</GUILayout>
